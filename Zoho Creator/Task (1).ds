						custom deluge script
						(
											disable Case_No_i;
										case_auto1 = inquiry_module[ID != 0].maximum(Auto_Number_i);
										// case_auto=input.Auto_Number;
										new_word = ifnull(case_auto1,0) + 1;
										input.Case_No_i = "C00" + new_word;
						)
					}
				}

			}
			VIN_History as "VIN History"
