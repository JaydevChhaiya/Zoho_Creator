			{
				type =  form
				form = inquiry_module
				record event = on add

				on user input of Dealership_Name_i
				{
					actions 
					{
						custom deluge script
						(
											fet = Dealer[ID != 0];
										deal_name = input.Dealership_Name_i;
										input.Date_i = zoho.currenttime;
										for each  var1 in fet
										{
											if(deal_name == var1)
											{
												input.Dealership_Code_i = var1.Dealership_Code;
												input.City_i = var1.City;
												input.Status_i = var1.Status;
